        "fund": "^1.0.0",
        "funding": "^1.0.9",
        "gulp-appdmg": "^1.0.3",
        "i18next": "^10.3.0",
        "i18next-xhr-backend": "^1.5.1",
        "jquery.filebrowser": "^0.8.2",
        "lru_map": "^0.3.3",
        "marked": "^0.8.0",
        "natives": "^1.1.6",
        "object-hash": "^1.3.1",
        "short-unique-id": "^1.1.1",
        "universal-ga": "^1.2.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "command-exists": "^1.2.8",
        "del": "^3.0.0",
        "follow-redirects": "^1.11.0",
        "fs-extra": "^6.0.1",
        "gulp": "^4.0.2",
        "gulp-concat": "~2.6.1",
        "gulp-debian": "~0.1.8",
        "gulp-git": "^2.10.1",
        "gulp-install": "^1.1.0",
        "gulp-rename": "~1.2.2",
        "gulp-watch": "^5.0.1",
        "gulp-zip": "^4.1.0",
        "inflection": "1.12.0",
        "jquery-ui-npm": "1.12.0",
        "karma": "^4.4.1",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-mocha": "^1.3.0",
        "karma-sinon": "^1.0.5",
        "karma-sinon-chai": "^1.3.4",
        "makensis": "^0.9.0",
        "mocha": "^5.2.0",
        "nw-builder": "^3.5.7",
        "os": "^0.1.1",
        "platform-dependent-modules": "0.0.14",
        "rpm-builder": "^1.2.1",
        "sinon": "^5.1.1",
        "sinon-chai": "^3.5.0",
        "targz": "^1.0.1",
        "temp": "^0.8.4"
