{
    "name": "emnode-configurator",
    "description": "Crossplatform configuration tool for Embedded node protocol.",
    "version": "0.0.0",
    "main": "main_nwjs.html",
    "bg-script": "js/eventPage.js",
    "default_locale": "en",
    "scripts": {
        "start": "node node_modules/gulp/bin/gulp.js debug",
        "_postinstall": "node ./node_modules/platform-dependent-modules/cli.js",
        "postinstall": "npm run _postinstall",
        "gulp": "gulp",
        "test": "karma start test/karma.conf.js"
    },
    "window": {
        "show": false,
        "icon": "images/enp_icon_128.png"
    },
    "repository": {
        "type": "git",
        "url": "github.com/embnode/ENConfigurator"
    },
    "webview": {
        "partitions": [{
            "name": "map",
            "accessible_resources": [
                "tabs/map.html",
                "js/tabs/map.js",
                "/images/icons/cf_icon_position.png"
            ]
        }]
    },
    "author": "The Embedded Node Configurator open source project.",
    "license": "GPL-3.0",
    "engines": {
        "node": "10.x"
    },
    "dependencies": {
        "i18next": "^19.3.4",
        "i18next-xhr-backend": "^3.2.2",
        "jquery.filebrowser": "^0.8.2",
        "marked": "^0.8.0",
        "natives": "^1.1.6",
        "universal-ga": "^1.2.0"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "command-exists": "^1.2.8",
        "del": "^5.1.0",
        "follow-redirects": "^1.11.0",
        "fs-extra": "^9.0.0",
        "gulp": "^4.0.2",
        "gulp-concat": "~2.6.1",
        "gulp-debian": "~0.1.8",
        "gulp-git": "^2.10.1",
        "gulp-install": "^1.1.0",
        "gulp-rename": "~2.0.0",
        "gulp-watch": "^5.0.1",
        "gulp-zip": "^5.0.1",
        "inflection": "1.12.0",
        "jquery-ui-npm": "1.12.0",
        "karma": "^4.4.1",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^3.1.0",
        "karma-mocha": "^1.3.0",
        "karma-sinon": "^1.0.5",
        "karma-sinon-chai": "^2.0.2",
        "makensis": "^0.20.3",
        "mocha": "^7.1.1",
        "nw-builder": "^3.5.7",
        "os": "^0.1.1",
        "platform-dependent-modules": "0.0.14",
        "rpm-builder": "^1.2.1",
        "sinon": "^9.0.1",
        "sinon-chai": "^3.5.0",
        "targz": "^1.0.1",
        "temp": "^0.9.1"
    },
    "optionalDependencies": {
        "gulp-appdmg": "^1.0.3"
    }
